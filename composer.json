{
    "name": "sinergi/config",
    "type": "library",
    "description": "PHP configurations loading",
    "keywords": ["Config", "Configurations", "Loading", "Management"],
    "homepage": "https://github.com/sinergi/config",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "Sinergi\\Config\\": "src/Config"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinergi\\Config\\Tests\\": ["tests/Config/Tests", "tests/Config/Tests/_includes"]
        }
    }
}
